   "source": [
    "#python 3.5.2\n",
    "# 숫자 엔개 중에서 최댓값 찾기를 재귀 호출\n",
    "\n",
    "def get_max_value(a):\n",
    "    max = a[0]; \n",
    "    for i in range(len(a)):\n",
    "        if(max < a[i]):\n",
    "            max = a[i]\n",
    "    \n",
    "    return max\n",
    "    #print(max)\n",
    "       \n",
    "        \n",
    "    \n",
    "\n",
    "list = [ 1,3,5,2, 4];\n",
    "print(get_max_value(list))\n"
   ]
  }
